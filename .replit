TWILIO_PHONE_NUMBER = "+41123456789"
EMAIL_SERVICE_PROVIDER = "sendgrid"
EMAIL_SERVICE_API_KEY = "SG.xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

